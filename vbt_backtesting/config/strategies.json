{
  "TrendFollow": {
    "status": "Active",
    "description": "Trend following strategies with moving average analysis",
    "max_concurrent_cases": 1,
    "cases": {
      "sma_basic": {
        "status": "Active",
        "priority": 1,
        "description": "Basic SMA20 crossover strategy",
        "entry": "(close > sma_10) & (close > sma_50)",
        "exit": "close < sma_10",
        "OptionType": "CALL",
        "StrikeType": "ATM"
      },
      "multi_sma_trend_call": {
        "status": "Active",
        "priority": 1,
        "description": "Multi-SMA trend entry (9>20>50>200), exit on close < SMA20",
        "entry": "(close > previous_day_high) & (close > sma_10) & (sma_10 > sma_50) & (sma_50 > sma_200) & (ltp_high > high)",
        "exit": "close < sma_10",
        "OptionType": "CALL",
        "StrikeType": "ATM",
        "position_size": "1"
      },
      "multi_sma_trend_put": {
        "status": "Active",
        "priority": 1,
        "description": "Multi-SMA trend PUT entry (9<20<50<200), exit on close > SMA20",
        "entry": "(close < previous_day_low) & (close < sma_10) & (sma_10 < sma_50) & (sma_50 < sma_200) & (ltp_low < low)",
        "exit": "close > sma_10",
        "OptionType": "PUT",
        "StrikeType": "ATM",
        "position_size": "1"
      }
    }
  },
  "EMA_TrendFollow": {
    "status": "Active",
    "description": "Trend following strategies with moving average analysis",
    "max_concurrent_cases": 1,
    "cases": {
      "ema_basic": {
        "status": "Active",
        "priority": 1,
        "description": "Basic SMA20 crossover strategy",
        "entry": "(close > ema_10) & (close > ema_20)",
        "exit": "close < ema_10",
        "OptionType": "CALL",
        "StrikeType": "ATM"
      },
      "multi_ema_trend_call": {
        "status": "Active",
        "priority": 1,
        "description": "Multi-SMA trend entry (9>20>50>200), exit on close < SMA20",
        "entry": "(close > ema_10) & (ema_10 > ema_20) & (ema_20 > ema_50) & (ema_50 > ema_200) & (ltp_high > high)",
        "exit": "close < ema_20",
        "OptionType": "CALL",
        "StrikeType": "ATM",
        "position_size": "1"
      },
      "multi_ema_trend_put": {
        "status": "Active",
        "priority": 1,
        "description": "Multi-SMA trend PUT entry (9<20<50<200), exit on close > SMA20",
       "entry": "(close < ema_10) & (ema_10 < ema_20) & (ema_20 < ema_50) & (ema_50 < ema_200) & (ltp_low < low)",
        "exit": "close > ema_20",
        "OptionType": "PUT",
        "StrikeType": "ATM",
        "position_size": "1"
      }
    }
  },
  "RSI_Reversal": {
    "status": "Active", 
    "description": "Mean reversion strategies using RSI and moving averages",
    "max_concurrent_cases": 1,
    "cases": {
      "RSI_Bounce_Call": {
        "status": "Active",
        "priority": 1,
        "description": "RSI oversold bounce, call when RSI < 30 and crosses above",
        "entry": "rsi_10 < 30 & close > sma_20",
        "exit": "rsi_10 > 50",
        "OptionType": "CALL",
        "StrikeType": "ATM"
      },
      "RSI_Drop_Put": {
        "status": "Active",
        "priority": 1,
        "description": "RSI overbought drop, put when RSI > 70 and reverses",
        "entry": "rsi_10 > 50 & close < sma_20",
        "exit": "rsi_10 < 30",
        "OptionType": "PUT",
        "StrikeType": "ATM"
      }
    }
  },
  "Breakout": {
    "status": "Active",
    "description": "Breakout strategies using VWAP and momentum",
    "max_concurrent_cases": 1,
    "cases": {
      "VWAP_Breakout_Call": {
        "status": "Active",
        "priority": 1,
        "description": "Price breaks above VWAP, with momentum",
        "entry": "(close > ema_20) & (ema_50 > ema_200)",
        "exit": "close < ema_10",
        "OptionType": "CALL",
        "StrikeType": "ATM"
      },
      "VWAP_Breakdown_Put": {
        "status": "Active",
        "priority": 1,
        "description": "Price breaks below VWAP, with weakness",
        "entry": "(close < ema_20) & (ema_50 < ema_200)",
        "exit": "close > ema_10",
        "OptionType": "PUT",
        "StrikeType": "ATM"
      }
    }
  }
}